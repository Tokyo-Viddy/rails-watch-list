<div class="controller">
  <h1>Let's make a new list</h1>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: "btn btn-primary mt-2" %>
    <%= link_to 'Back to list index', lists_path, class: 'btn btn-secondary mt-2' %>
  <% end %>
</div>
